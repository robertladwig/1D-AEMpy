@article{woolway_northern_2019,
	title = {Northern {Hemisphere} {Atmospheric} {Stilling} {Accelerates} {Lake} {Thermal} {Responses} to a {Warming} {World}},
	volume = {46},
	issn = {1944-8007},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2019GL082752},
	doi = {10.1029/2019GL082752},
	abstract = {Climate change, in particular the increase in air temperature, has been shown to influence lake thermal dynamics, with climatic warming resulting in higher surface temperatures, stronger stratification, and altered mixing regimes. Less studied is the influence on lake thermal dynamics of atmospheric stilling, the decrease in near-surface wind speed observed in recent decades. Here we use a lake model to assess the influence of atmospheric stilling, on lake thermal dynamics across the Northern Hemisphere. From 1980 to 2016, lake thermal responses to warming have accelerated as a result of atmospheric stilling. Lake surface temperatures and thermal stability have changed at respective rates of 0.33 and 0.38 °C/decade, with atmospheric stilling contributing 15\% and 27\% of the calculated changes, respectively. Atmospheric stilling also resulted in a lengthening of stratification, contributing 23\% of the calculated changes. Our results demonstrate that atmospheric stilling has influenced lake thermal responses to warming.},
	language = {en},
	number = {21},
	urldate = {2020-09-02},
	journal = {Geophysical Research Letters},
	author = {Woolway, R. Iestyn and Merchant, Christopher J. and Hoek, Jamon Van Den and Azorin‐Molina, Cesar and Nõges, Peeter and Laas, Alo and Mackay, Eleanor B. and Jones, Ian D.},
	year = {2019},
	keywords = {climate, lake, wind},
	pages = {11983--11992},
}

