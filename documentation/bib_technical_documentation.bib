@article{ulloa_mechanical_2018,
	title = {Mechanical energy budget and mixing efficiency for a radiatively heated ice-covered waterbody},
	volume = {852},
	issn = {0022-1120, 1469-7645},
	url = {https://www.cambridge.org/core/journals/journal-of-fluid-mechanics/article/abs/mechanical-energy-budget-and-mixing-efficiency-for-a-radiatively-heated-icecovered-waterbody/BE298A2A0CD2A74BF6FF1C898B60E968},
	doi = {10.1017/jfm.2018.587},
	abstract = {Ice-covered waterbodies are far from being quiescent systems. In this paper, we investigate ice-covered freshwater basins heated by solar radiation that penetrates across waters with temperatures below or near the temperature of maximum density. In this scenario, solar radiation sets a radiative buoyancy flux, \${\textbackslash}unicode[STIX]\{x1D6F7\}\_\{r\}\$, that forces increments of temperature/density in the upper fluid volume, which can become gravitationally unstable and drive convection. The goal of this study is twofold. We first focus on formulating the mechanical energy budget, putting emphasis on the conversion of \${\textbackslash}unicode[STIX]\{x1D6F7\}\_\{r\}\$ to available potential energy, \$E\_\{a\}\$. We find that \$E\_\{a\}\$ results from a competition among \${\textbackslash}unicode[STIX]\{x1D6F7\}\_\{r\}\$ and the irreversible mixing controlled by the diapycnal and the laminar mixing rates, respectively. Secondly, and based on the above result, we introduce an integral formulation of the mixing efficiency to quantify the rate of mixing over the relevant time scale \${\textbackslash}unicode[STIX]\{x1D70F\}\$, \${\textbackslash}unicode[STIX]\{x1D702\}\_\{c\}{\textbackslash}equiv {\textbackslash}unicode[STIX]\{x0394\}E\_\{b,{\textbackslash}unicode[STIX]\{x1D70F\}\}/E\_\{r,{\textbackslash}unicode[STIX]\{x1D70F\}\}\$, where \${\textbackslash}unicode[STIX]\{x0394\}E\_\{b,{\textbackslash}unicode[STIX]\{x1D70F\}\}\$ and \$E\_\{r,{\textbackslash}unicode[STIX]\{x1D70F\}\}\$ are the change of background potential energy and the time-integrated \${\textbackslash}unicode[STIX]\{x1D6F7\}\_\{r\}\$ over \${\textbackslash}unicode[STIX]\{x1D70F\}\$. The above definition is applied to estimate \${\textbackslash}unicode[STIX]\{x1D702\}\_\{c\}\$ for the first time, finding an approximate value of \${\textbackslash}unicode[STIX]\{x1D702\}\_\{c\}{\textbackslash}approx 0.65\$. This result suggests that radiatively heated ice-covered waterbodies might be subject to high mixing rates. Overall, the present work provides a framework to examine energetics and mixing in ice-covered waters.},
	language = {en},
	urldate = {2023-07-19},
	journal = {Journal of Fluid Mechanics},
	author = {Ulloa, Hugo N. and Wüest, Alfred and Bouffard, Damien},
	month = oct,
	year = {2018},
	keywords = {convection in cavities, ice sheets, turbulent mixing},
	pages = {R1},
}

@article{kirillin_physics_2012-1,
	title = {Physics of seasonally ice-covered lakes: a review},
	volume = {74},
	issn = {1420-9055},
	shorttitle = {Physics of seasonally ice-covered lakes},
	url = {https://doi.org/10.1007/s00027-012-0279-y},
	doi = {10.1007/s00027-012-0279-y},
	abstract = {Recently, the attention to the ice season in lakes has been growing remarkably amongst limnological communities, in particular, due to interest in the response of mid- and high-latitude lakes to global warming. We review the present advances in understanding the governing physical processes in seasonally ice-covered lakes. Emphasis is placed on the general description of the main physical mechanisms that distinguish the ice-covered season from open water conditions. Physical properties of both ice cover and ice-covered water column are considered. For the former, growth and decay of the seasonal ice, its structure, mechanical and optical properties are discussed. The latter subject deals with circulation and mixing under ice. The relative contribution of the two major circulation drivers, namely heat release from sediment and solar heating, is used for classifying the typical circulation and mixing patterns under ice. In order to provide a physical basis for lake ice phenology, the heat transfer processes related to formation and melting of the seasonal ice cover are discussed in a separate section. Since the ice-covered period in lakes remains poorly investigated to date, this review aims at elaborating an effective strategy for future research based on modern field and modeling methods.},
	language = {en},
	number = {4},
	urldate = {2023-07-19},
	journal = {Aquatic Sciences},
	author = {Kirillin, Georgiy and Leppäranta, Matti and Terzhevik, Arkady and Granin, Nikolai and Bernhardt, Juliane and Engelhardt, Christof and Efremova, Tatyana and Golosov, Sergey and Palshin, Nikolai and Sherstyankin, Pavel and Zdorovennova, Galina and Zdorovennov, Roman},
	month = oct,
	year = {2012},
	keywords = {Freezing lakes, Ice physics, Lake hydrodynamics, Physical limnology, Review},
	pages = {659--682},
}

